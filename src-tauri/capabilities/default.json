{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "dialog:default",
    "fs:default",
    "fs:write-all",
    "shell:default",
    "shell:allow-open"
  ]
}